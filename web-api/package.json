{
  "name": "web-api",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/server.js --exec babel-node",
    "build": "gulp build",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "predeploy": "yarn build",
    "deploy": "gcloud beta functions deploy apiService --stage-bucket record-scrobbler-src --trigger-http --source=build --region=us-central1",
    "deploy-local": "functions deploy apiService --trigger-http --local-path=build",
    "clean": "gulp clean"
  },
  "author": "Filipe Tavares",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/trace-agent": "^2.2.0",
    "@google/cloud-debug": "^0.9.3",
    "axios": "^0.16.2",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-graphql": ">0.6.4",
    "express-jwt": "^5.3.0",
    "graphql": ">0.10.0",
    "graphql-relay": ">0.5.1",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "redis": "^2.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-uglify": "^3.0.0"
  }
}
