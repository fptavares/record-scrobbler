{
  "name": "discogs-service",
  "version": "1.0.0",
  "description": "DRS Discogs Service",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/server.js --exec babel-node",
    "start-db-emulator": "gcloud beta emulators datastore start --no-store-on-disk",
    "build": "gulp build",
    "deploy-gcloud": "gcloud beta functions deploy discogsService --stage-bucket record-scrobbler-src --trigger-http --local-path=build --region=us-central1",
    "deploy-local": "functions deploy discogsService --trigger-http --source=build",
    "clean": "gulp clean"
  },
  "author": "Filipe Tavares",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/datastore": "^1.1.0",
    "@google-cloud/trace-agent": "^2.2.0",
    "@google/cloud-debug": "^0.9.3",
    "disconnect": "^1.0.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.0.1",
    "morgan": "^1.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-uglify": "^3.0.0"
  }
}
